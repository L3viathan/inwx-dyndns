[live]
url = https://api.domrobot.com/xmlrpc/
username = your_username
password = your_password

[ote]
url = https://api.ote.domrobot.com/xmlrpc/
username = your_ote_account_username
password = your_ote_account_password


[automatic_nameserver_entry_update]
domain = example.com
subdomain = laptop.dyndns.example.com
default_ip = 2001:db8:d4:a000:f174:7b5c:1ffd:f7f3

[nameserver_backup]
json_backup_file = ~/nameserver.backup.json
pickle_backup_file = ~/nameserver.backup.pickle

[invoices_folder]
invoices_folder = ~/
